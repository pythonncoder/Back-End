package first;

import java.util.Scanner;

public class ReverseString {

	public static void main(String[] args) {
		
		Scanner Sc = new Scanner(System.in);
		System.out.println("Enter the String:");
		String  str = Sc.nextLine();
		
		String[] str1 = null;
		 str1 = str.split(" ");
		 System.out.println(str1);
		
		

	}

}
