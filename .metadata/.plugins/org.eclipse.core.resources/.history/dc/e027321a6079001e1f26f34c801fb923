package first;

import java.util.ArrayList;
import java.util.LinkedList;
import java.util.List;

public class ArrayList1  {

	public static void main(String[] args) {
		
		
		List<String> l = new ArrayList<String>();
		
		List<String> a = new LinkedList<String>();
		a.add("praveen"); // adding of elements
		a.add("venky");
		
		l.add(0,"Naveen"); //addin of elements in index wise order
		l.add("nknkn");
		l.addAll(a);   //add the collection of data
		l.addAll(4, a); // Adding of collection in index order
		l.clear();    // clear the list of values
		
		
		for (String l1:l) {
			System.out.println(l1);
		}
		
		

	}

}
